{
  "name": "express-react-boilerplate",
  "version": "0.0.1",
  "description": "An example for react, webpack, babel commbined to express",
  "main": "server.js",
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-3"
    ]
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "scripts": {
    "start": "nodemon server.js --exec babel-node --config ./config/nodemon.json",
    "test": "mocha --compilers js:babel-core/register ./test/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ole3021/express-react-boilerplate.git"
  },
  "author": "Oliver Wang <ole3021@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ole3021/express-react-boilerplate/issues"
  },
  "homepage": "https://github.com/ole3021/express-react-boilerplate#readme",
  "dependencies": {
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-3": "^6.11.0",
    "compression": "^1.6.2",
    "express": "^4.14.0",
    "intl": "^1.2.4",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "html-webpack-plugin": "^2.22.0",
    "mocha": "^3.0.2",
    "nodemon": "^1.10.0",
    "standard": "^7.1.2",
    "supertest": "^2.0.0",
    "webpack": "^2.1.0-beta.21",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2"
  }
}
